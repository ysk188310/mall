[32m+[m[32m */[m
[32m+[m
[32m+[m[32m::-webkit-file-upload-button {[m
[32m+[m[32m  -webkit-appearance: button; /* 1 */[m
[32m+[m[32m  font: inherit; /* 2 */[m
